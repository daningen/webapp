{
  "name": "se.dbwebb.trafik",
  "displayName": "Trafik",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --config webpack.dev.config.js && cordova run browser",
    "build": "webpack --config webpack.prod.config.js && cordova run browser",
    "style": "sass base.scss style.min.css",
    "sass-compile": "sass base.scss style.min.css --style compressed"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "sass": "^1.49.8",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      }
    },
    "platforms": [
      "browser",
      "ios"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@mithril-icons/font-awesome": "^1.2.0",
    "heatmap.js": "^2.0.5",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.3.2",
    "leaflet-heatmap": "file:node_modules/leaflet-heatmap",
    "mithril": "^2.0.4"
  }
}
